
@{
    ViewData["Title"] = "OfferteUpload";
}

<h2>OfferteUpload</h2>

<form asp-controller="Offer" asp-action="OfferUpload" method="post"
      enctype="multipart/form-data">

    <input type="file" name="file" accept=".doc, .docx" />
    <button type="submit" class="upload">Upload File</button>
</form>